{
  "name": "@vinicunca/unocss-preset",
  "type": "module",
  "version": "1.31.0",
  "description": "Opinionated UnoCSS preset",
  "author": "praburangki<https://github.com/praburangki>",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/unocss-preset-vinicunca#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/unocss-preset-vinicunca.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/unocss-preset-vinicunca/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-preset-animation",
    "unocss-preset-fluid"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "stub": "tsdown --stub",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:update": "vitest --update",
    "publish:ci": "pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "@vinicunca/perkakas": "catalog:core",
    "unocss": "catalog:base"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@unocss/core": "catalog:uno",
    "@unocss/preset-icons": "catalog:uno",
    "@unocss/preset-mini": "catalog:uno",
    "@unocss/preset-typography": "catalog:uno",
    "@unocss/preset-web-fonts": "catalog:uno",
    "@unocss/preset-wind4": "catalog:uno",
    "@unocss/transformer-directives": "catalog:uno",
    "@unocss/transformer-variant-group": "catalog:uno",
    "defu": "catalog:core"
  }
}
