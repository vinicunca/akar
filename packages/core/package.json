{
  "name": "akar",
  "type": "module",
  "version": "1.12.0",
  "description": "Vue UI components",
  "author": "Vinicunca Contributors (https://github.com/vinicunca)",
  "license": "MIT",
  "homepage": "https://github.com/vinicunca/akar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinicunca/akar.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/akar/issues"
  },
  "keywords": [
    "ui",
    "vue",
    "accessibility",
    "headless",
    "nuxt",
    "components",
    "unstyled",
    "akar"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./constant": {
      "import": "./src/constant.ts"
    },
    "./date": {
      "import": "./src/date.ts"
    },
    "./namespaced": {
      "import": "./src/namespaced.ts"
    },
    "./nuxt": {
      "import": "./src/nuxt/index.ts"
    },
    "./resolver": {
      "import": "./src/resolver/index.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./constant": {
        "import": "./dist/constant.js",
        "require": "./dist/constant.cjs"
      },
      "./date": {
        "import": "./dist/date.js",
        "require": "./dist/date.cjs"
      },
      "./namespaced": {
        "import": "./dist/namespaced/index.mjs",
        "require": "./dist/namespaced/index.cjs"
      },
      "./nuxt": {
        "import": "./dist/nuxt/index.mjs",
        "require": "./dist/nuxt/index.cjs"
      },
      "./resolver": {
        "import": "./dist/resolver/index.mjs",
        "require": "./dist/resolver/index.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "!src/**/*.story.vue",
    "!src/**/*.test.*",
    "!src/**/_*.vue",
    "!src/**/__snapshots__",
    "!src/**/stories",
    "!src/**/story",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "pnpm type-check && pnpm build-only",
    "build-only": "tsdown && tsx scripts/postbuild.ts",
    "watch": "tsdown --watch",
    "type-check": "vue-tsc -p tsconfig.check.json --noEmit",
    "test": "vitest",
    "test-update": "vitest -u",
    "pub:release": "pnpm publish --access public --no-git-checks",
    "check-sizes": "node scripts/check-sizes.js",
    "track-sizes": "node scripts/track-sizes.js",
    "analyze-components": "node scripts/analyze-components.js",
    "trend-analysis": "node scripts/trend-analysis.js"
  },
  "peerDependencies": {
    "vue": ">= 3.2.0"
  },
  "dependencies": {
    "@floating-ui/dom": "catalog:core",
    "@floating-ui/vue": "catalog:core",
    "@internationalized/date": "catalog:core",
    "@internationalized/number": "catalog:core",
    "@tanstack/vue-virtual": "catalog:core",
    "@vinicunca/perkakas": "catalog:core",
    "@vueuse/core": "catalog:core",
    "@vueuse/shared": "catalog:core",
    "aria-hidden": "catalog:core",
    "defu": "catalog:core"
  },
  "devDependencies": {
    "@iconify/vue": "catalog:core",
    "@testing-library/dom": "catalog:core",
    "@testing-library/jest-dom": "catalog:core",
    "@testing-library/user-event": "catalog:core",
    "@testing-library/vue": "catalog:core",
    "@tsconfig/node18": "catalog:core",
    "@types/jsdom": "catalog:core",
    "@types/node": "catalog:core",
    "@vitejs/plugin-vue": "catalog:core",
    "@vitest/coverage-istanbul": "catalog:core",
    "@vue/test-utils": "catalog:core",
    "@vue/tsconfig": "catalog:core",
    "happy-dom": "catalog:core",
    "jsdom": "catalog:core",
    "tsdown": "catalog:core",
    "vite": "catalog:core",
    "vitest": "catalog:core",
    "vitest-axe": "catalog:core",
    "vitest-canvas-mock": "catalog:core",
    "vue": "catalog:core",
    "vue-component-type-helpers": "catalog:core",
    "vue-tsc": "catalog:core"
  }
}
